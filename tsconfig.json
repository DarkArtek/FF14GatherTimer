{
  "compilerOptions": {
    "sourceMap": true,
    // TSはECMAScript 5に変換
    "target": "es5",
    // webpack 2 以降 または rollup を使用している場合、ツリーシェイキングを活用するために
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ]
  }, "include": [ "resources/assets/js/**/*" ]
}

